@page "/"
@namespace Blazor.AdaptiveCards.SampleApp.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Blazor.AdaptiveCards samples</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="css/outlook.css" rel="stylesheet" />
    <link href="css/prism.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="_content/Blazor.AdaptiveCards/adaptiveCardsJsInterop.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/prism.js" data-manual></script>
    <script src="js/codeBlockJsInterop.js" data-manual></script>
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
</head>
<body>
    <app>
        @(await Html.RenderComponentAsync<App>(RenderMode.Server))
    </app>

    <script src="_framework/blazor.server.js"></script>
    <script>
        AOS.init({
            once: true
        });
    </script>
</body>
</html>
